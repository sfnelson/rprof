<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:p="urn:import:nz.ac.vuw.ecs.rprofs.client">
	<ui:style type="nz.ac.vuw.ecs.rprofs.client.views.impl.ReportPanel.Style">
	.panel {}
	.background {
		position: absolute;
		width: 100%;
		height: 100%;
		color: transparent;
		z-index: -1;
	}
	.background span, .background div {
		height: 100%;
		padding-top: 0;
		padding-bottom: 0;
	}
	.heading {
		background-color: #ddd;
		border-bottom: solid 1px #ccc;
	}
	.heading span {
		padding-top: 0.5ex;
		padding-bottom: 0.5ex;
		border-color: #bbb;
		text-align: left;
	}
	.even {
		background-color: #eef;
	}
	.container {
		position: absolute;
		top: 1px;
		bottom: 0;
		left: 0;
		right: 0;
		overflow: auto;
	}
	.refresh {
		display: inline-block;
		padding: 0.1ex;
		margin: 0.2ex 0.5em;
		float: right;
		text-decoration: none;
		color: black;
	}
	</ui:style>
	<g:FlowPanel styleName="{style.panel}">
		<g:FlowPanel ui:field="children" styleName="{style.container} list">
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 