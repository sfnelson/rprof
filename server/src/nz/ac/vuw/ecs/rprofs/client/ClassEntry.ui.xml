<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="nz.ac.vuw.ecs.rprofs.client.ClassEntry.Style">
		.packageName, .className, .methods, .instances, .equals, .hash {
			display: inline-block;
			font-size: small;
			margin-right: 1em;
			overflow: hidden;
			width: 5em;
			padding: 0.2ex;
			border-right: solid 1px #eee;
		}
		.packageName {
			margin-left: 1em;
			width: 15em;
			text-align: left ! important;
		}
		.className {
			width: 15em;
			text-align: left ! important;
		}
		.methods {}
		.instances {}
		.equals {}
		.hash {}
		.defaultPackage {
			font-style: italic;
			color: #777;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:InlineLabel ui:field="packageLabel" styleName="{style.packageName}"></g:InlineLabel>
		<g:InlineLabel ui:field="classLabel" styleName="{style.className}"></g:InlineLabel>
		<g:InlineLabel ui:field="methodsLabel" styleName="{style.methods}"></g:InlineLabel>
		<g:InlineLabel ui:field="instancesLabel" styleName="{style.instances}"></g:InlineLabel>
		<g:InlineLabel ui:field="equalsLabel" styleName="{style.equals}"></g:InlineLabel>
		<g:InlineLabel ui:field="hashLabel" styleName="{style.hash}"></g:InlineLabel>
	</g:HTMLPanel>
</ui:UiBinder> 