<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.1.1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.1.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='profiler'>
  <inherits name='com.google.gwt.user.User' />
  <inherits name="com.google.web.bindery.requestfactory.RequestFactory"/>
  <inherits name='com.google.gwt.activity.Activity' />
  <inherits name='com.google.gwt.uibinder.UiBinder' />
  
  <set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true" />

  <entry-point class='nz.ac.vuw.ecs.rprofs.client.AppEntryPoint'/>
  
  <source path='client'/>
  
  <replace-with class="nz.ac.vuw.ecs.rprofs.client.FactoryImpl">
    <when-type-is class="nz.ac.vuw.ecs.rprofs.client.Factory" />
  </replace-with>
</module>
