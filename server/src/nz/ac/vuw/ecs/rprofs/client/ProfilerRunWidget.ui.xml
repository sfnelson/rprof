<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.row {
			width: 100%;
			position: relative;
			overflow: auto;
		}
		.date, .className {
			display: inline-block;
			padding: 0.5ex 0.5em;
			width: 12em;
			white-space: nowrap;
			overflow: hidden;
		}
		.className {
			width: 20em;
		}
		.buttonPanel {
			float: right;
		}
		.button {}
	</ui:style>
	<g:HTMLPanel styleName='{style.row}'>
		<g:InlineLabel ui:field="started" styleName='{style.date}' />
		<g:InlineLabel ui:field="stopped" styleName='{style.date}' />
		<g:InlineLabel ui:field="program" styleName='{style.className}' />
		<div class="{style.buttonPanel}">
			<g:Button ui:field="inspect" styleName='{style.button}'>Inspect</g:Button>
			<g:Button ui:field="delete" styleName='{style.button}'>Delete</g:Button>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 